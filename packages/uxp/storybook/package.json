  "alias": {
    "@storybook/react": "./src/@storybook/react",
    "@storybook/addon-actions": "./src/@storybook/addon-actions",
    "--@react-spectrum/button": "@react-spectrum/button/src/uxp"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "parcel": "nightly",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@react-spectrum/button": "^3.3.0",
    "@react-spectrum/provider": "^3.1.2",
    "@react-spectrum/theme-default": "^3.1.0",
    "react": "16.13.1",
    "react-dom": "16.13.1"
  },
  "bundledDependencies": ["react", "react-dom"]
}
