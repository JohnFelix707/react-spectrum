{
  "name": "rs3uxp-plugin",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "main": "dist/index.html",
  "scripts": {
    "watch:parcel": "rm -rf ../../../.parcel-cache && parcel serve src/index.html --target main",
    "watch-:uxp": "sleep 20 && yarn uxp",
    "watch": "run-p watch:*",
    "uxp": "../../../../torq-native/build/out/bin/macos/v8/nativeui/Debug/demo.app/Contents/MacOS/demo --extension dist"
  },
  "targets": {
    "main": {
      "minify": false
    }
